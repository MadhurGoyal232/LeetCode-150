class Solution {
    public void merge(int[] nums1, int m, int[] nums2, int n) {
        if(n==0){
            return;
        }
        
        int j = n-1;
        int k = m+n-1;
        while(j>=0){
            if(m>0 && nums1[m-1] > nums2[j]){
            
                nums1[k] = nums1[m-1];
                m--;
                k--;
            }else{
                nums1[k] = nums2[j];
                j--;
                k--;
            }
        }
    }
}

