class Solution {
    int min = Integer.MAX_VALUE;
    int profit = 0;
    public int maxProfit(int[] prices) {
        
        for(int x : prices){

            if(x<min){
                min = x;
                // profit = 0;
            }else{
                profit = Math.max(profit, x-min);
            }
        }
        return profit;
    }
}
