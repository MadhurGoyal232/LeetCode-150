class Solution {
    public boolean isValidSudoku(char[][] board) {
        HashSet<String> set = new HashSet<>();
        for(int i=0;i<board.length;i++){
            for(int j=0;j<board[0].length;j++){
                if(board[i][j]== '.'){
                    continue;
                }

                String row =  board[i][j] + "at " + i + "row position";
                String col =  board[i][j] + "at" + j + "col position";
                String within =  board[i][j] + "at" + i/3 + "-" + j/3 + " within";


                if(set.contains(row) || set.contains(col) || set.contains(within)){
                    return false;
                }
                set.add(row); 
                set.add(col);
                set.add(within);

            }
        }
        return true;
    }
}
