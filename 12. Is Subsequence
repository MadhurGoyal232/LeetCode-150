class Solution {
    public boolean isSubsequence(String s, String t) {
        if (s.length() == 0) return true;

        int n = t.length();
        int a = 0;
        for(int i=0;i<n;i++){
            if(s.charAt(a) == t.charAt(i)){
                a++;
            }
            if(a == s.length()){
                break;
            }
        }
        return a == s.length() ? true: false;
    }
}
class Solution {
    public boolean isSubsequence(String s, String t) {
        int i=0;
        int j=0;
        int x = 0;
        while( j< t.length() && i< s.length()){

            if(s.charAt(i) == t.charAt(j)){
                i++;
                x++;
                j++;
            }
            else{
                j++;
            }
        }
        return x == s.length();
        
    }
}
