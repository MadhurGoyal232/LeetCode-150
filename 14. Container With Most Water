class Solution {
    public int maxArea(int[] height) {
        int max = Integer.MIN_VALUE;
        int n = height.length-1;

        int left = 0;
        int right = n;
        while(left<right){
            
            int h = Math.min(height[left] , height[right]);
            max = Math.max(max , (right-left) * h);
           if(height[left] < height[right]){
            left++;
           }else{
            right--;
           }
        }
        return max;
    }
}
