class Solution {
    public int pSum(int[][] grid, int dp[][], int m , int n){
        if(m<0 || n<0){
            return (int)1e9;
        }
        if(m==0 && n==0){
            return grid[0][0];
        }
        if(dp[m][n]!= -1){
            return dp[m][n];
        }

        int left = grid[m][n] + pSum(grid, dp, m, n-1);
        int up = grid[m][n] + pSum(grid, dp, m-1, n);

        return dp[m][n] = Math.min(left , up);
    }
    public int minPathSum(int[][] grid) {
        int dp[][] = new int[grid.length][grid[0].length];
        for(int i[] : dp){
            Arrays.fill(i, -1);
        }

        return pSum(grid, dp, grid.length-1 , grid[0].length-1);
    }
}
