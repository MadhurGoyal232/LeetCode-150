class Solution {
    public int findPeakElement(int[] nums) {

        int low = 0;
        int high = nums.length-1;
        while(low<high){
            int mid= low+(high-low)/2;
            if(nums[mid]< nums[mid+1]){
                low = mid+1;
            }else{
                high = mid;
            }
    
        }
        return low;
        // for(int i=1;i<nums.length;i++){
        //     if(nums[i]<nums[i-1]){
        //         return i-1;
            
        //     }
        // }
        // return nums.length-1;
    }
}
